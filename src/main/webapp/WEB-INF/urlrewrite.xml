        <from>/login_success</from>
        <to type="redirect">tendersHome</to>
    </rule>
    <rule>
        <condition type="user-in-role">SELLER</condition>
        <from>/login_success</from>
        <to type="redirect">tendersHome</to>
    </rule>
    <rule>
        <condition type="user-in-role">MODERATOR</condition>
        <from>/login_success</from>
